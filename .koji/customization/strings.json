{
  "strings": {
    "fontFamily": "https://fonts.googleapis.com/css?family=Viga&display=swap",
    "title": "Run N Switch",
    "playerSize": 75,
    "playerMoveSpeed": 750,
    "gravitySpeed": 1100,
    "shieldDuration": 15,
    "speedDuration": 5,
    "speedMultiplication": 1.5,
    "gameCountdown": 3,
    "desktop_instructions1": "Switch the gravity to save the player!",
    "desktop_instructions2": "Hit Spacebar to switch!",
    "desktop_instructions3": "Don't hit bombs and keep running!",
    "mobile_instructions1": "Switch the gravity to save player!",
    "mobile_instructions2": "Touch to switch!",
    "mobile_instructions3": "Don't hit bombs and keep running!",
    "defaultPlayerName": "Enter your name",
    "playButtonText": "PLAY NOW",
    "leaderboardButtonText": "LEADERBOARD",
    "gameOverText": "GAME OVER",
    "shieldText": "SHIELD",
    "speedText": "SPEED",
    "gameStartedFloatingText": "Game started!",
    "emailInputEnabled": false,
    "mainMenuBackgroundSize": ""
  },
  "@@editor": [
    {
      "key": "strings",
      "name": "Game Settings",
      "icon": "ðŸ’¡",
      "source": "strings.json",
      "fields": [
        {
          "key": "fontFamily",
          "name": "Font Family",
          "description": "Google fonts link to font to use in the game",
          "type": "text"
        },
        {
          "key": "title",
          "name": "Game Title",
          "description": "The title that is displayed in the start menu",
          "type": "text"
        },
        {
          "key": "playerSize",
          "name": "Player Size",
          "type": "range",
          "typeOptions": {
            "min": 50,
            "max": 150,
            "step": 2
          }
        },
        {
          "key": "playerMoveSpeed",
          "name": "Player Movement Speed",
          "type": "range",
          "typeOptions": {
            "min": 500,
            "max": 1250,
            "step": 2
          }
        },
        {
          "key": "gravitySpeed",
          "name": "Gravity Speed",
          "description": "Speed at which player switches/flips.",
          "type": "range",
          "typeOptions": {
            "min": 700,
            "max": 2000,
            "step": 2
          }
        },
        {
          "key": "shieldDuration",
          "name": "Shield Duration",
          "type": "range",
          "typeOptions": {
            "min": 4,
            "max": 25,
            "step": 1
          }
        },
        {
          "key": "speedDuration",
          "name": "Speed Duration",
          "type": "range",
          "typeOptions": {
            "min": 1,
            "max": 20,
            "step": 1
          }
        },
        {
          "key": "speedMultiplication",
          "name": "Speed Multiplication",
          "type": "range",
          "typeOptions": {
            "min": 0.5,
            "max": 2.5,
            "step": 0.1
          }
        },
        {
          "key": "gameCountdown",
          "name": "Game Time Countdown",
          "description": "0 for no countdown.",
          "type": "range",
          "typeOptions": {
            "min": 0,
            "max": 5,
            "step": 1
          }
        },
        {
          "key": "desktop_instructions1",
          "name": "Desktop Game Instructions 1",
          "type": "text"
        },
        {
          "key": "desktop_instructions2",
          "name": "Desktop Game Instructions 2",
          "type": "text"
        },
        {
          "key": "desktop_instructions3",
          "name": "Desktop Game Instructions 3",
          "type": "text"
        },
        {
          "key": "mobile_instructions1",
          "name": "Mobile Game Instructions 1",
          "type": "text"
        },
        {
          "key": "mobile_instructions2",
          "name": "Mobile Game Instructions 2",
          "type": "text"
        },
        {
          "key": "mobile_instructions3",
          "name": "Mobile Game Instructions 3",
          "type": "text"
        },
        {
          "key": "defaultPlayerName",
          "name": "Default Player Name",
          "description": "Default player name that appears in the main menu.",
          "type": "text"
        },
        {
          "key": "playButtonText",
          "name": "Play Button Text",
          "description": "Label on the Play Button",
          "type": "text"
        },
        {
          "key": "leaderboardButtonText",
          "name": "Leaderboard Button Text",
          "description": "Label on the Leaderboard Button",
          "type": "text"
        },
        {
          "key": "gameOverText",
          "name": "Game Over Text",
          "type": "text"
        },
        {
          "key": "shieldText",
          "name": "Shield Text",
          "type": "text"
        },
        {
          "key": "speedText",
          "name": "Speed Text",
          "type": "text"
        },
        {
          "key": "emailInputEnabled",
          "name": "Enable E-Mail Input",
          "description": "Enable/Disable E-Mail input while submitting score.",
          "type": "boolean"
        },
        {
          "key": "mainMenuBackgroundSize",
          "name": "Main Menu Background Size",
          "description": "Empty for tiled background. Use CSS background-size properties only. Eg. cover",
          "type": "text"
        }
      ]
    }
  ]
}